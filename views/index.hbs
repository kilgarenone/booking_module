<h1>{{title}}</h1>
{{#each dates }}
  <div>{{@key}}</div>
  <div>
    <div class="day-header">
      <div>Mon</div>
      <div>Tue</div>
      <div>Wed</div>
      <div>Thu</div>
      <div>Fri</div>
      <div>Sat</div>
      <div>Sun</div>
    </div>
    <div class="month">
      {{#each this }}
        <div class="day {{disableDay this}}" data-date="{{this}}">{{day this}}</div>
      {{/each}}
    </div>
  </div>
{{/each}}
